<h1>Listing users</h1>

<table>
  <tr>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email%></td>
    <td><%= link_to 'Show', show_user_path(user)%></td>
    <td><%= link_to 'Edit', edit_user_registration_path(user),id: "edit_#{user.id}"%></td>
    <td><%= link_to 'Destroy', users_path, method: :delete, data: { confirm: 'Are you sure?' },id: "delete_#{user.id}"%></td>



  </tr>
<% end %>
</table>

<br />
